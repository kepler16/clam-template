{
	"name": "client",
	"version": "0.0.1",
	"scripts": {
		"dev:server": "npx shadow-cljs server",
		"watch:client": "npx shadow-cljs watch client",
		"watch:css": "yarn compile:css",
		"watch": "npm-run-all --parallel watch:*",
		"compile:client": "npx shadow-cljs compile client",
		"compile:css": "npx tailwindcss-cli@latest build ./resources/css/tailwind.css -o ../vercel/public/static/css/tailwind.css",
		"compile": "npm-run-all --parallel compile:*",
		"release:client": "npx shadow-cljs release client",
		"release:css": "NODE_ENV=production npx tailwindcss-cli@latest build ./resources/css/tailwind.css -o ../vercel/public/static/css/tailwind.css",
		"release": "npm-run-all --parallel release:*"
	},
	"dependencies": {},
	"devDependencies": {}
}
